      [
        {
          "type": "TEXT",
          "content": "program reads iconv module configuration files and writes"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "a fast-loading gconv module configuration cache file."
        }
      ]
    ]
  },
  {
    "type": "macro",
