  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TH",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " INTRO 8 2007-10-23 \"Linux\" \"Linux Programmer's Manual\""
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NAME"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "intro "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " introduction to administration and privileged commands"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Section 8 of the manual describes commands"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "which either can be or are used only by the superuser,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "like system-administration commands, daemons,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and hardware-related commands."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "As with the commands described in Section 1, the commands described"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "in this section terminate with an exit status that indicates"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "whether the command succeeded or failed."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "See"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " intro (1)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "for more information."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NOTES"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " Authors and copyright conditions"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Look at the header of the manual page source for the author(s) and copyright"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "conditions."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Note that these can be different from page to page!"
        }
      ]
    ]
  }
