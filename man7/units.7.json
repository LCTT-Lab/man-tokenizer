      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " Binary prefixes"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The binary prefixes resemble the decimal ones,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "but have an additional "
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": "i"
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(and \"Ki\" starts with a capital "
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": "K"
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": ")."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The names are formed by taking the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "first syllable of the names of the decimal prefix with roughly the same"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "size, followed by \"bi\" for \"binary\"."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".RS",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TS",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "l l l."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Prefix\tName\tValue"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Ki\tkibi\t2^10 = 1024"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Mi\tmebi\t2^20 = 1048576"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Gi\tgibi\t2^30 = 1073741824"
        }
      ],
      [
        {
          "type": "TEXT",
