      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "ASCII is the American Standard Code for Information Interchange."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It is a 7-bit code."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Many 8-bit codes (e.g., ISO 8859-1) contain ASCII as their lower half."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The international counterpart of ASCII is known as ISO 646-IRV."
        }
      ]
    ]
  },
