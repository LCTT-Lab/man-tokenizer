  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " expression"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is false (i.e., compares equal to zero),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " assert ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "prints an error message to standard error"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and terminates the program by calling"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " abort (3)."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The error message includes the name of the file and function containing the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " assert ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "call, the source code line number of the call, and the text of the argument;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "something like:"
        }
      ]
    ]
  },
  {
