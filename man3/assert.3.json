  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If the macro"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " NDEBUG"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is defined at the moment"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " <assert.h>"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "was last included, the macro"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " assert ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "generates no code, and hence does nothing at all."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It is not recommended to define"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " NDEBUG"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "if using"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " assert ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to detect error conditions since the software"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "may behave non-deterministically."
        }
      ]
    ]
  },
