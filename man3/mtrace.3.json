      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\" FIXME: The marking is different from that in the glibc manual,"
        }
      ],
      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\" markings in glibc manual are more detailed:"
        }
      ],
      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\""
