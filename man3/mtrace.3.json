      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " MALLOC_TRACE ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "which should contain the pathname of a file in which"
        }
