      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " x"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "times"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
