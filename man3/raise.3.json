      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " raise ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "will return only after the signal handler has returned."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
