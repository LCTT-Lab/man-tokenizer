        },
        {
          "type": "TEXT",
          "content": " longjmp (3)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "from a signal handler or modifying the handling of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SIGALRM"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "while sleeping will cause undefined results."
        }
      ]
    ]
