  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "On Linux,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sleep ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is implemented via"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " nanosleep (2)."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "See the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " nanosleep (2)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "man page for a discussion of the clock used."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
