  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sleep ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "causes the calling thread to sleep either until"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the number of real-time seconds specified in"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " seconds"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "have elapsed or until a signal arrives which is not ignored."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " RETURN VALUE"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Zero if the requested time has elapsed,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or the number of seconds left to sleep,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "if the call was interrupted by a signal handler."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
