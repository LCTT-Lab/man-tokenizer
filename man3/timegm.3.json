        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " timelocal ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "takes the local timezone into account when doing the conversion, while"
        }
