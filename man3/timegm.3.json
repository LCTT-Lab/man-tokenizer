        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " timegm ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "takes the input value to be Coordinated Universal Time (UTC)."
        }
