        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    uid_t    pw_uid;       /* user ID */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    gid_t    pw_gid;       /* group ID */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    char    *pw_gecos;     /* user information */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    char    *pw_dir;       /* home directory */"
        }
      ],
      [
        {
