    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " __ppc_mdoio ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "provides a hint that performance will probably be improved if shared"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "resources dedicated to the executing processor are released until all"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "outstanding storage accesses to caching-inhibited storage have been"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "completed."
        }
      ]
    ]
  },
