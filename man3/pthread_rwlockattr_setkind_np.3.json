        {
          "type": "TEXT",
          "content": " pthread_rwlockattr_getkind_np ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "always succeeds."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "On error,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
