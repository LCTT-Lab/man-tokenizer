  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strfmon ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function formats the specified monetary amount"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "according to the current locale"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and format specification"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " format"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and places the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "result in the character array"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "of size"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
