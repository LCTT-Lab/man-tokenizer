  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strfmon_l ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function performs the same task,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "but uses"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the locale specified by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " locale ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The behavior of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strfmon_l ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is undefined if"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " locale"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is the special locale object"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " LC_GLOBAL_LOCALE"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(see"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " duplocale (3))"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or is not a valid locale object handle."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
