        },
        {
          "type": "TEXT",
          "content": " nbytes"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "are read."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Reading starts at offset"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " *basep ,"
