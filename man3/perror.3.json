  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "First (if"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
