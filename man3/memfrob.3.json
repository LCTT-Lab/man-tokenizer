        {
          "type": "TEXT",
          "content": " memfrob ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function encrypts the first "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "In"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P bytes of the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "memory area "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Is"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P by exclusive-ORing each character with the number"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "42."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The effect can be reversed by using"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " memfrob ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "on the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "encrypted memory area."
        }
      ]
    ]
  },
  {
