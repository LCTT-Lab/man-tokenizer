  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Early glibc2 is buggy and returns 0 in the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " millitm"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "field;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "glibc 2.1.1 is correct again."
        }
      ]
    ]
  },
  {
