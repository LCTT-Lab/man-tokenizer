  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "    sigemptyset(&set);"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    sigaddset(&set, SIGQUIT);"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    sigaddset(&set, SIGUSR1);"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    s = pthread_sigmask(SIG_BLOCK, &set, NULL);"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    if (s != 0)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "        handle_error_en(s, \"pthread_sigmask\");"
        }
      ]
    ]
  },
