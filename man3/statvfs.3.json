      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "struct statvfs {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long  f_bsize;    /* Filesystem block size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long  f_frsize;   /* Fragment size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    fsblkcnt_t     f_blocks;   /* Size of fs in f_frsize units */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    fsblkcnt_t     f_bfree;    /* Number of free blocks */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    fsblkcnt_t     f_bavail;   /* Number of free blocks for"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "                                  unprivileged users */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    fsfilcnt_t     f_files;    /* Number of inodes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    fsfilcnt_t     f_ffree;    /* Number of free inodes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    fsfilcnt_t     f_favail;   /* Number of free inodes for"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "                                  unprivileged users */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long  f_fsid;     /* Filesystem ID */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long  f_flag;     /* Mount flags */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long  f_namemax;  /* Maximum filename length */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "};"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
