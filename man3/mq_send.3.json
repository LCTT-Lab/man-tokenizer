      ],
      [
        {
          "type": "TEXT",
          "content": "struct timespec {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    time_t tv_sec;        /* seconds */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    long   tv_nsec;       /* nanoseconds */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "};"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
