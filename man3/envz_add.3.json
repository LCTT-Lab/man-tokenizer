        {
          "type": "TEXT",
          "content": "="
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": " is considered"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to be the value."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If there is no "
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": "="
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": ", the value is taken to be NULL."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(While the value in case of a trailing "
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": "="
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": " is the empty string \"\".)"
        }
      ]
    ]
  },
