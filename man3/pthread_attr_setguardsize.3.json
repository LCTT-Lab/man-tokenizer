  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " guardsize"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is greater than 0,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "then for each new thread created using"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " attr"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the system allocates an additional region of at least"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " guardsize"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "bytes at the end of the thread's stack to act as the guard area"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "for the stack (but see BUGS)."
        }
      ]
    ]
  },
  {
