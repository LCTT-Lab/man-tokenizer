        },
        {
          "type": "TEXT",
          "content": " severity"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "argument of the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fmtmsg (3)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "By default, that function knows only how to"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "print messages for severity 0-4 (with strings (none), HALT,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "ERROR, WARNING, INFO)."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This call attaches the given string"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to the given value"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " severity ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is NULL, the severity class with the numeric value"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " severity"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is removed."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It is not possible to overwrite or remove one of the default"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "severity classes."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The severity value must be nonnegative."
        }
      ]
    ]
