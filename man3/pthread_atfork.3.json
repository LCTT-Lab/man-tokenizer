        {
          "type": "TEXT",
          "content": " child"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "specifies a handler that is executed in the child process after"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
