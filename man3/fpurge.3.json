      ],
      [
        {
          "type": "TEXT",
          "content": "clears the buffers of the given stream."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "For output streams this discards any unwritten output."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "For input streams this discards any input read from the underlying object"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "but not yet obtained via"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
