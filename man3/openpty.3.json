        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " winp"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "arguments, if not NULL,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "will determine the terminal attributes and window size of the slave"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "side of the pseudoterminal."
        }
      ]
    ]
  },
  {
