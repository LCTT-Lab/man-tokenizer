  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pthread_spin_lock ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function locks the spin lock referred to by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " lock ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If the spin lock is currently unlocked,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the calling thread acquires the lock immediately."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If the spin lock is currently locked by another thread,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the calling thread spins, testing the lock until it becomes available,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "at which point the calling thread acquires the lock."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Calling"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pthread_spin_lock ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "on a lock that is already held by the caller"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or a lock that has not been initialized with"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pthread_spin_init (3)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "results in undefined behavior."
        }
      ]
    ]
  },
  {
