      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pthread_spin_unlock ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function unlocks the spin lock referred to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " lock ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If any threads are spinning on the lock,"
        }
      ],
