        },
        {
          "type": "TEXT",
          "content": " str"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "into substrings separated by the delimiter"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " delim ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "For example, one might use this on a UNIX search path with"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "delimiter "
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": ":"
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": "."
        }
      ]
    ]
  },
