  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " argz_count ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "counts the number of strings, that is,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the number of null bytes ("
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "ESCAPE",
          "content": "\\\\"
        },
        {
