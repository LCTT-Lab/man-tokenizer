        },
        {
          "type": "TEXT",
          "content": " argz_add ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "adds the string"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " str"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "at the end of the array"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " *argz ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and updates"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " *argz"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " *argz_len ."
        }
      ]
    ]
  },
