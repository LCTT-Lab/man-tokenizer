  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " strerror_r()"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strerror_r ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function is similar to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strerror (),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "but is"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "thread safe."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This function is available in two versions:"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "an XSI-compliant version specified in POSIX.1-2001"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(available since glibc 2.3.4, but not POSIX-compliant until glibc 2.13),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and a GNU-specific version (available since glibc 2.0)."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The XSI-compliant version is provided with the feature test macros"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "settings shown in the SYNOPSIS;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "otherwise the GNU-specific version is provided."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If no feature test macros are explicitly defined,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "then (since glibc 2.4)"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " _POSIX_C_SOURCE"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is defined by default with the value"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "200112L, so that the XSI-compliant version of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strerror_r ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is provided by default."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The XSI-compliant"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strerror_r ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is preferred for portable applications."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It returns the error string in the user-supplied buffer"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " buf"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "of length"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " buflen ."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The GNU-specific"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " strerror_r ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns a pointer to a string containing the error message."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This may be either a pointer to a string that the function stores in"
        }
      ],
      [
        {
