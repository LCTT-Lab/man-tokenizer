  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "When successful,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tcsetpgrp ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns 0."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Otherwise, it returns "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "1, and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " errno"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is set appropriately."
        }
      ]
    ]
  },
