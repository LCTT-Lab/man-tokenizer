  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tcgetpgrp ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns the process group ID of the foreground process group on the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "terminal associated to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fd ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "which must be the controlling terminal of the calling process."
        }
      ]
    ]
  },
  {
    "type": "comment",
    "lines": [
      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\" The process itself may be a background process."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tcsetpgrp ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "makes the process group with process group ID"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pgrp"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the foreground process group on the terminal associated to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fd ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "which must be the controlling terminal of the calling process,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and still be associated with its session."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Moreover,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pgrp"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "must be a (nonempty) process group belonging to"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the same session as the calling process."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tcsetpgrp ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is called by a member of a background process group in its session,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and the calling process is not blocking or ignoring"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SIGTTOU ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "a"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SIGTTOU"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "signal is sent to all members of this background process group."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " RETURN VALUE"
