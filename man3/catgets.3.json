  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " catgets ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function is available only in libc.so.4.4.4c and above."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The Jan 1987 X/Open Portability Guide specifies a more subtle"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "error return:"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " message"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is returned if the message catalog specified by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " catalog"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is not available, while an empty string is returned"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "when the message catalog is available but does not contain"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the specified message."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "These two possible error returns seem to be discarded in SUSv2"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "in favor of always returning"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " message ."
        }
      ]
    ]
