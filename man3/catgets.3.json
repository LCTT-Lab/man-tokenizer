          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " catgets ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns a pointer to an internal buffer area"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "containing the null-terminated message string."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "On failure,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " catgets ()"
        }
      ],
