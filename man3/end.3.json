  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " CONFORMING TO"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Although these symbols have long been provided on most UNIX systems,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "they are not standardized; use with caution."
        }
      ]
    ]
  },
  {
    "type": "macro",
