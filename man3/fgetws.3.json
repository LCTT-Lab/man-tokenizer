  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The behavior of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fgetws ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "depends on the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " LC_CTYPE"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "category of the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "current locale."
        }
      ]
    ]
  },
  {
    "type": "macro",
