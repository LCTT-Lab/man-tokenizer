  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fgetws ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function is the wide-character equivalent"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "of the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fgets (3)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It reads a string of at most "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "In"
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "1"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P wide characters into the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "wide-character array pointed to by "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Iws"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and adds a terminating null wide character (L"
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "ESCAPE",
          "content": "\\\\"
        },
        {
          "type": "TEXT",
          "content": "0"
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": ")."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It stops reading wide characters after it has encountered and"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "stored a newline wide character."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It also stops when end of stream is reached."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
