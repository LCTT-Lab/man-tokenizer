        {
          "type": "TEXT",
          "content": " fgetws ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function, if successful, returns "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Iws"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If end of stream"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "was already reached or if an error occurred, it returns NULL."
        }
