  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ttyname ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns a pointer to the null-terminated pathname of the terminal device"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "that is open on the file descriptor "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Ifd"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P, or NULL on error"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(for example, if "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Ifd"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is not connected to a terminal)."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The return value may point to static data, possibly overwritten by the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "next call."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ttyname_r ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "stores this pathname in the buffer"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " buf"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "of length"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " buflen ."
