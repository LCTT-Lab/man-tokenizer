  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " wordexp ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "performs a shell-like expansion of the string"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and returns the result in the structure pointed to by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " p ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The data type"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " wordexp_t"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is a structure that at least has the fields"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordc ,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordv ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_offs ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The field"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordc"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is a"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " size_t"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "that gives the number of words in the expansion of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The field"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordv"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is a"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " \"char"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "**\""
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "that points to the array of words found."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The field"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_offs"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "of type"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " size_t"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is sometimes (depending on"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " flags ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "see below) used to indicate the number of initial elements in the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordv"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "array that should be filled with NULLs."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " wordfree ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "frees the allocated memory again."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "More precisely, it does not free"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "its argument, but it frees the array"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordv"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and the strings that points to."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " The string argument"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Since the expansion is the same as the expansion by the shell (see"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sh (1))"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "of the parameters to a command, the string"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "must not contain characters that would be illegal in shell command"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "parameters."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "In particular, there must not be any unescaped"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "newline or |, &, ;, <, >, (, ), {, } characters"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "outside a command substitution or parameter substitution context."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If the argument"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "contains a word that starts with an unquoted comment character #,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "then it is unspecified whether that word and all following words"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "are ignored, or the # is treated as a non-comment character."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " The expansion"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The expansion done consists of the following stages:"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "tilde expansion (replacing ~user by user's home directory),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "variable substitution (replacing $FOO by the value of the environment"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "variable FOO), command substitution (replacing $(command) or "
        },
        {
          "type": "ESCAPE",
          "content": "\\`"
        },
        {
          "type": "TEXT",
          "content": "command"
        },
        {
          "type": "ESCAPE",
          "content": "\\`"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "by the output of command), arithmetic expansion, field splitting,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "wildcard expansion, quote removal."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The result of expansion of special parameters"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "($@, $*, $#, $?, $"
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": ", $$, $!, $0) is unspecified."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Field splitting is done using the environment variable $IFS."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If it is not set, the field separators are space, tab and newline."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " The output array"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The array"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_wordv"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "contains the words found, followed by a NULL."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " The flags argument"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " flag"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "argument is a bitwise inclusive OR of the following values:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " WRDE_APPEND"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Append the words found to the array resulting from a previous call."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " WRDE_DOOFFS"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Insert"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " we_offs"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "initial NULLs in the array"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
