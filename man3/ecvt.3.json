  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ecvt ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function converts "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Inumber"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P to a null-terminated"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "string of "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Indigits"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P digits (where "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Indigits"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is reduced to a"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "system-specific limit determined by the precision of a"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " double ),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and returns a pointer to the string."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The high-order digit is nonzero, unless"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " number"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is zero."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The low order digit is rounded."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The string itself does not contain a decimal point; however,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the position of the decimal point relative to the start of the string"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is stored in "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "I*decpt"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "A negative value for "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "I*decpt"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P means that"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the decimal point is to the left of the start of the string."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If the sign of"
        }
      ],
      [
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Inumber"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is negative, "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "I*sign"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is set to a nonzero value,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "otherwise it is set to 0."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " number"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is zero, it is unspecified whether "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "I*decpt"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is 0 or 1."
        }
      ]
    ]
  },
