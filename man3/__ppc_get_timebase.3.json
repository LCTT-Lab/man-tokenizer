  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "    tb2 = __ppc_get_timebase();"
        }
      ]
    ]
  },
  {
    "type": "empty",
    "lines": [
      [

      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "    if (tb2 > tb1) {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "        diff = tb2 "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " tb1;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    } else {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "        /* Treat Time Base Register overflow.  */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "        diff = (MAX_TB "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " tb2) + tb1;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    }"
        }
      ]
    ]
  },
  {
    "type": "empty",
