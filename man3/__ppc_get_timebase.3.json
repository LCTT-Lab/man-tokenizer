  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "/* Maximum value of the Time Base Register: 2^60 "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " 1."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "   Source: POWER ISA.  */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "#define MAX_TB 0xFFFFFFFFFFFFFFF"
        }
      ]
    ]
  },
  {
    "type": "empty",
    "lines": [
