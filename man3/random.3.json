        },
        {
          "type": "TEXT",
          "content": " random ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The state array "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Istate"
        },
        {
          "type": "ESCAPE",
