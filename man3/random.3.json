  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " random ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function uses a nonlinear additive feedback random"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "number generator employing a default table of size 31 long integers to"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "return successive pseudo-random numbers in"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the range from 0 to "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "BRAND_MAX"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "R."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The period of this random number generator is very large, approximately"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " \"16"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "*"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "((2^31)"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "1)\" ."
        }
      ]
    ]
  },
  {
    "type": "macro",
