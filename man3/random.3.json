  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " srandom ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function sets its argument as the seed for a new"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "sequence of pseudo-random integers to be returned by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " random ()."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "These sequences are repeatable by calling"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " srandom ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "with the same"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "seed value."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If no seed value is provided, the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " random ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is automatically seeded with a value of 1."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
