  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " initstate ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function allows a state array "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Istate"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P to"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "be initialized for use by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " random ()."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The size of the state array"
        }
      ],
      [
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "In"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is used by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " initstate ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to decide how sophisticated a"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "random number generator it should use"
        },
        {
          "type": "ESCAPE",
          "content": "\\(em"
        },
        {
          "type": "TEXT",
          "content": "the larger the state array,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the better the random numbers will be."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Current \"optimal\" values for the size of the state array "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "In"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P are"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "8, 32, 64, 128, and 256 bytes; other amounts will be rounded down to"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the nearest known amount."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Using less than 8 bytes results in an error."
        }
      ],
      [
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Iseed"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is the seed for the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "initialization, which specifies a starting point for the random number"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "sequence, and provides for restarting at the same point."
        }
      ]
    ]
  },
