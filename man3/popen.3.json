      [
        {
          "type": "TEXT",
          "content": "argument is a pointer to a null-terminated string which must contain"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "either the letter "
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
        },
        {
          "type": "TEXT",
          "content": "r"
        },
        {
          "type": "ESCAPE",
          "content": "\\(aq"
