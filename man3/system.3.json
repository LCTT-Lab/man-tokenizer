    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " system ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "does not affect the wait status of any other children."
        }
      ]
    ]
  },
  {
