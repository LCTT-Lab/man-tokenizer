  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pshared"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is nonzero, then the semaphore is shared between processes,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and should be located in a region of shared memory (see"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " shm_open (3),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mmap (2),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " shmget (2))."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(Since a child created by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fork (2)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "inherits its parent's memory mappings, it can also access the semaphore.)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Any process that can access the shared memory region"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "can operate on the semaphore using"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sem_post (3),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sem_wait (3),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and so on."
        }
      ]
    ]
  },
  {
    "type": "macro",
