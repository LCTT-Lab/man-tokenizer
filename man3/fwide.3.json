  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "When "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Imode"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is zero, the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fwide ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function determines the current"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "orientation of "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Istream"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It returns a positive value if "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Istream"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "wide-character oriented, that is, if wide-character I/O is permitted but char"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "I/O is disallowed."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It returns a negative value if "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Istream"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is byte oriented"
        },
        {
          "type": "ESCAPE",
          "content": "\\(em"
        },
        {
          "type": "TEXT",
          "content": "that is,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "if char I/O is permitted but wide-character I/O is disallowed."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns zero if "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Istream"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P has no orientation yet; in this case the next"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "I/O operation might change the orientation (to byte oriented if it is a char"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "I/O operation, or to wide-character oriented if it is a wide-character I/O"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "operation)."
        }
      ]
