        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Imode"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is greater than 0, or"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to byte oriented if "
