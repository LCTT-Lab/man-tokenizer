      [
        {
          "type": "TEXT",
          "content": "will use."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Glibc will try the path prefix"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " P_tmpdir"
