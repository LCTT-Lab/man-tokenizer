      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tmpfile ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function opens a unique temporary file"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "in binary read/write (w+b) mode."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The file will be automatically deleted when it is closed or the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "program terminates."
        }
      ]
    ]
  },
