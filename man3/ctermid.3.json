  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ctermid ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns a string which is the pathname for the current"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "controlling terminal for this process."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is NULL,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "a static buffer is used, otherwise"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "points to a buffer used to hold the terminal pathname."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The symbolic constant"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " L_ctermid"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is the maximum number of characters in the returned pathname."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
