      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ERANGE ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or raise an"
        }
