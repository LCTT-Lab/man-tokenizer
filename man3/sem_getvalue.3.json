        {
          "type": "TEXT",
          "content": " sem_getvalue ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns 0 on success;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "on error, "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "1 is returned and"
        }
