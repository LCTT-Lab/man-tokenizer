        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mkfifoat ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "function operates in exactly the same way as"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mkfifo (),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "except for the differences described here."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
