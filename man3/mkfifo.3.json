        },
        {
          "type": "TEXT",
          "content": "P."
        }
      ],
      [
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Imode"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P specifies the FIFO's permissions."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It is modified by the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "process's "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Bumask"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P in the usual way: the permissions of the created"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "file are "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "B("
        },
        {
          "type": "ESCAPE",
