    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pthread_yield ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is intended for use with real-time scheduling policies (i.e.,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SCHED_FIFO"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SCHED_RR )."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Use of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pthread_yield ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "with nondeterministic scheduling policies such as"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SCHED_OTHER"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is unspecified and very likely means your application design is broken."
        }
      ]
    ]
