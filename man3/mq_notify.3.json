        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mq_notify ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "allows the calling process to register or unregister for delivery of"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "an asynchronous notification when a new message arrives on"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the empty message queue referred to by the message queue descriptor"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mqdes ."
        }
      ]
