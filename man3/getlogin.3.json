      [
        {
          "type": "TEXT",
          "content": "returns a pointer to the username when successful,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and NULL on failure, with"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
