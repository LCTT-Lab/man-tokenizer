  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If direct access to the FPU control word is still needed, the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " _FPU_GETCW"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " _FPU_SETCW"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "macros from"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " <fpu_control.h>"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "can be used."
        }
      ]
    ]
  },
