      [
        {
          "type": "TEXT",
          "content": "command, and then sets the environment variables"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " LOCPATH"
        }
