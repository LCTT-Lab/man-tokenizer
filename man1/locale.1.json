        {
          "type": "TEXT",
          "content": " $HOME/.locale"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "directory, then tests the result with the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " date (1)"
        }
