      [
        {
          "type": "TEXT",
          "content": "supporting 32-bit IDs."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The glibc"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " setresuid ()"
