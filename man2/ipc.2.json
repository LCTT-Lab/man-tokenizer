  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "ipc "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " System V IPC system calls"
        }
