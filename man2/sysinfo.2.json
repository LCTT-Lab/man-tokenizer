  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TH",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " SYSINFO 2 2017-09-15 \"Linux\" \"Linux Programmer's Manual\""
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NAME"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "sysinfo "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " return system information"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " SYNOPSIS"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " #include <sys/sysinfo.h>"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".BI",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " \"int sysinfo(struct sysinfo *\" info );"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sysinfo ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns certain statistics on memory and swap usage,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "as well as the load average."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Until Linux 2.3.16,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sysinfo ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returned information in the following structure:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " +4n"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "struct sysinfo {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    long uptime;             /* Seconds since boot */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long loads[3];  /* 1, 5, and 15 minute load averages */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long totalram;  /* Total usable main memory size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long freeram;   /* Available memory size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long sharedram; /* Amount of shared memory */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long bufferram; /* Memory used by buffers */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long totalswap; /* Total swap space size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long freeswap;  /* Swap space still available */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned short procs;    /* Number of current processes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    char _f[22];             /* Pads structure to 64 bytes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "};"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "In the above structure, the sizes of the memory and swap fields"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "are given in bytes."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Since Linux 2.3.23 (i386) and Linux 2.3.48"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(all architectures) the structure is:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " +4n"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "struct sysinfo {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    long uptime;             /* Seconds since boot */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long loads[3];  /* 1, 5, and 15 minute load averages */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long totalram;  /* Total usable main memory size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long freeram;   /* Available memory size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long sharedram; /* Amount of shared memory */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long bufferram; /* Memory used by buffers */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long totalswap; /* Total swap space size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long freeswap;  /* Swap space still available */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned short procs;    /* Number of current processes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long totalhigh; /* Total high memory size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned long freehigh;  /* Available high memory size */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    unsigned int mem_unit;   /* Memory unit size in bytes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    char _f[20"
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "2*sizeof(long)"
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "sizeof(int)];"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "                             /* Padding to 64 bytes */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "};"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "In the above structure,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "sizes of the memory and swap fields are given as multiples of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mem_unit"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "bytes."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " RETURN VALUE"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "On success,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sysinfo ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns zero."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "On error, "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "1 is returned, and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " errno"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is set to indicate the cause of the error."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " ERRORS"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " EFAULT"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " info"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is not a valid address."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " VERSIONS"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sysinfo ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "first appeared in Linux 0.98.pl6."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " CONFORMING TO"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "This function is Linux-specific, and should not be used in programs"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "intended to be portable."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NOTES"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "All of the information provided by this system call is also available via"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /proc/meminfo"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /proc/loadavg ."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " SEE ALSO"
        }
      ]
    ]
  },
