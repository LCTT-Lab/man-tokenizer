        }
      ],
      [
        {
          "type": "TEXT",
          "content": "on any protection key which has been assigned to an address"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "range by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
