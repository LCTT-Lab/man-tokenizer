  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The kernel guarantees that the contents of the hardware rights"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "register (PKRU) will be preserved only for allocated protection"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "keys."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Any time a key is unallocated (either before the first call"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returning that key from"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pkey_alloc ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or after it is freed via"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
