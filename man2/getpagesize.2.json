  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " getpagesize ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "returns the number of bytes in a memory page,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "where \"page\" is a fixed-length block,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the unit for memory allocation and file mapping performed by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
