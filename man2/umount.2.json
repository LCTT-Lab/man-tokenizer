        },
        {
          "type": "TEXT",
          "content": " ENOTBLK"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "when called with something other than a block device."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "In Linux 0.98p4, a call "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Iumount(dir)"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P was added, in order to"
        }
