      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " setuid32 ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "supporting 32-bit IDs."
        }
      ],
      [
