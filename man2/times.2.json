        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " times ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "just returns a function result."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "However, POSIX does not specify this behavior, and most"
        }
