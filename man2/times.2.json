        }
      ],
      [
        {
          "type": "TEXT",
          "content": "struct tms {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    clock_t tms_utime;  /* user time */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    clock_t tms_stime;  /* system time */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    clock_t tms_cutime; /* user time of children */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    clock_t tms_cstime; /* system time of children */"
        }
      ],
