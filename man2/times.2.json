        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " CLOCKS_PER_SEC ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "not the clock ticks used by"
