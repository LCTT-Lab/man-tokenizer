  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " acct ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "system call enables or disables process accounting."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If called with the name of an existing file as its argument,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "accounting is turned on,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and records for each terminating process are appended to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " filename"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "as it terminates."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "An argument of NULL causes accounting to be turned off."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
