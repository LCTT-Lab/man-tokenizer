  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "        _DEFAULT_SOURCE"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    In glibc 2.19 and 2.20:"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "        _DEFAULT_SOURCE || (_XOPEN_SOURCE && _XOPEN_SOURCE"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "<"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "500)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    Up to and including glibc 2.19:"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "        _BSD_SOURCE || (_XOPEN_SOURCE && _XOPEN_SOURCE"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "<"
        },
        {
