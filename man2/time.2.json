    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " #include <time.h>"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
