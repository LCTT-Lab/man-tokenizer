  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " func"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is negative or 0, and no daemon has been started, then"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " bdflush ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "enters the daemon code and never returns."
        }
      ]
    ]
  },
