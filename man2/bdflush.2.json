  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "this system call is deprecated and does nothing."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It is likely to disappear altogether in a future kernel release."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Nowadays, the task performed by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " bdflush ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is handled by the kernel"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pdflush"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "thread."
        }
      ]
    ]
  },
