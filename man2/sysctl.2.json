  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "struct __sysctl_args {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    int    *name;    /* integer vector describing variable */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    int     nlen;    /* length of this vector */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    void   *oldval;  /* 0 or address where to store old value */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    size_t *oldlenp; /* available room for old value,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "                        overwritten by actual size of old value */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    void   *newval;  /* 0 or address of new value */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    size_t  newlen;  /* size of new value */"
        }
      ],
      [
        {
          "type": "TEXT",
