  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sched_setparam ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "sets the scheduling parameters associated with the scheduling policy"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "for the thread whose thread ID is specified in "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Ipid"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Ipid"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P is zero, then"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the parameters of the calling thread are set."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The interpretation of"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the argument "
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "Iparam"
        },
        {
          "type": "ESCAPE",
          "content": "\\f"
        },
        {
          "type": "TEXT",
          "content": "P depends on the scheduling"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "policy of the thread identified by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " pid ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "See"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sched (7)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "for a description of the scheduling policies supported under Linux."
        }
