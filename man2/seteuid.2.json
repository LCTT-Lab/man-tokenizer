        },
        {
          "type": "TEXT",
          "content": " EPERM"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "In the case of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " seteuid ():"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the calling process is not privileged (does not have the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " CAP_SETUID"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "capability in its user namespace) and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " euid"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "does not match the current real user ID, current effective user ID,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or current saved set-user-ID."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".IP",
          "translatable": true
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "In the case of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " setegid ():"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the calling process is not privileged (does not have the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " CAP_SETGID"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "capability in its user namespace) and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " egid"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "does not match the current real group ID, current effective group ID,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or current saved set-group-ID."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
