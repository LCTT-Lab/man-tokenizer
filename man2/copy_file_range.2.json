      ],
      [
        {
          "type": "TEXT",
          "content": "bytes of data from file descriptor"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fd_in"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to file descriptor"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fd_out ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "overwriting any data that exists within the requested range of the target file."
        }
      ]
    ]
  },
  {
