      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " fd_in"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "starting from the file offset, and the file offset is"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "adjusted by the number of bytes copied."
        }
      ]
    ]
  },
