  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " copy_file_range ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "system call performs an in-kernel copy between two file descriptors"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "without the additional cost of transferring data from the kernel to user space"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and then back into the kernel."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It copies up to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " len"
