  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "If"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " off_in"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is NULL, then bytes are read from"
