  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " io_destroy ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "system call"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "will attempt to cancel all outstanding asynchronous I/O operations against"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ctx_id ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "will block on the completion of all operations"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "that could not be canceled, and will destroy the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " ctx_id ."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
