        },
        {
          "type": "TEXT",
          "content": " EACCES"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Search permission is denied for one of the directories in"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the path prefix of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " path"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(see also"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
