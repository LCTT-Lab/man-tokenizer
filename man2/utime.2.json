  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " Note:"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "modern applications may prefer to use the interfaces described in"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
