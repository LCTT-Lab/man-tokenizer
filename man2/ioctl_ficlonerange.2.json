  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "ioctl_ficlonerange, ioctl_ficlone "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " share some the data of one file with another file"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
