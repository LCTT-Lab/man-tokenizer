      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " addrlen"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "argument should be initialized to indicate"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the amount of space (in bytes) pointed to by"
        }
      ],
      [
