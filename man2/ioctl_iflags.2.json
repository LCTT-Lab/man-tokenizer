  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "ioctl_iflags "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " ioctl() operations for inode flags"
        }
