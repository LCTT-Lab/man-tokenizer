          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " BCACHE"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Same as"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
