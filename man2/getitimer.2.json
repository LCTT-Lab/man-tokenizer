  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " +4n"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "struct itimerval {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    struct timeval it_interval; /* Interval for periodic timer */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    struct timeval it_value;    /* Time until next expiration */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "};"
        }
      ]
    ]
  },
  {
    "type": "empty",
    "lines": [
      [

      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "struct timeval {"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    time_t      tv_sec;         /* seconds */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "    suseconds_t tv_usec;        /* microseconds */"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "};"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
  },
