  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The function"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " getitimer ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "places the current value of the timer specified by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " which"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "in the buffer pointed to by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " curr_value ."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " it_value"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "substructure is populated with the amount of time remaining until"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the next expiration of the specified timer."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This value changes as the timer counts down, and will be reset to"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " it_interval"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "when the timer expires."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If both fields of"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " it_value"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "are zero, then this timer is currently disarmed (inactive)."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
