  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "POSIX.1-2001 says that"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " setitimer ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "should fail if a"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tv_usec"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "value is specified that is outside of the range 0 to 999999."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "However, in kernels up to and including 2.6.21,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Linux does not give an error, but instead silently"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "adjusts the corresponding seconds value for the timer."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "From kernel 2.6.22 onward,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "this nonconformance has been repaired:"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "an improper"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " tv_usec"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "value results in an"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " EINVAL"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "error."
        }
      ]
    ]
  },
