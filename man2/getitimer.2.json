      [
        {
          "type": "TEXT",
          "content": "approximately 99.42 days."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Since Linux 2.6.16,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the kernel uses a different internal representation for times,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and this ceiling is removed."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
