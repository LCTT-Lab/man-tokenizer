  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "These system calls must be used instead of the simple assignment"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or data-transfer operations that are used to access the PCI MMIO"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "memory areas mapped to user space on the Linux System z platform."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The address specified by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mmio_addr"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "must belong to a PCI MMIO memory page mapping in the caller's address space,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and the data being written or read must not cross a page boundary."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " length"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "value cannot be greater than the system page size."
        }
      ]
    ]
  },
