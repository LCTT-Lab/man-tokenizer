  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The watch descriptor is returned by later"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " read (2)s"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "from the inotify file descriptor."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "These reads fetch"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " inotify_event"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "structures (see"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " inotify (7))"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "indicating filesystem events;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the watch descriptor inside this structure identifies"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the object for which the event occurred."
        }
      ]
    ]
  },
