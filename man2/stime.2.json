    "lines": [
      [
        {
          "type": "TEXT",
          "content": "stime "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " set time"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
