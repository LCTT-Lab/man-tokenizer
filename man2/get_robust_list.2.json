  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " get_robust_list ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "system call returns the head of the robust futex list of the thread"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "whose thread ID is specified in"
        }
      ],
      [
        {
