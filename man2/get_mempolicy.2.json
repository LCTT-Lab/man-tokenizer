      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " flags"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is specified as 0,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "then information about the calling thread's default policy"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(as set by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " set_mempolicy (2))"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is returned, in the buffers pointed to by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mode"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " nodemask ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The value returned in these arguments"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "may be used to restore the thread's policy to its state at"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the time of the call to"
