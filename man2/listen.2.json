    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " listen ()"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "marks the socket referred to by"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sockfd"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "as a passive socket, that is, as a socket that will"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "be used to accept incoming connection requests using"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " accept (2)."
        }
