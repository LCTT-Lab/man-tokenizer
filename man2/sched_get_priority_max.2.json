  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "sched_get_priority_max, sched_get_priority_min  "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " get static priority range"
        }
      ]
    ]
  },
  {
