  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "They are typically created by:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
