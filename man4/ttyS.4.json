    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " +4n"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/ttyS0 c 4 64 # base address 0x3f8"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/ttyS1 c 4 65 # base address 0x2f8"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/ttyS2 c 4 66 # base address 0x3e8"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/ttyS3 c 4 67 # base address 0x2e8"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "chown root:tty /dev/ttyS[0"
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "3]"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
  },
