  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TH",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " HD 4 2017-09-15 \"Linux\" \"Linux Programmer's Manual\""
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NAME"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "hd "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " MFM/IDE hard disk devices"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hd*"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "devices are block devices to access MFM/IDE hard disk drives"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "in raw mode."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The master drive on the primary IDE controller (major device"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "number 3) is"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hda ;"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "the slave drive is"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hdb ."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The master drive of the second controller (major device number 22)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hdc"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and the slave is"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hdd ."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "General IDE block device names have the form"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".BI",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hd X"
        },
        {
          "type": "ESCAPE",
          "content": "\\c"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ", or"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".BI",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hd XP"
        },
        {
          "type": "ESCAPE",
          "content": "\\c"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ", where"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " X"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is a letter denoting the physical drive, and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " P"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is a number denoting the partition on that physical drive."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The first form,"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".BI",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hd X"
        },
        {
          "type": "ESCAPE",
          "content": "\\c"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ", is used to address the whole drive."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Partition numbers are assigned in the order the partitions"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "are discovered, and only nonempty, nonextended partitions"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "get a number."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "However, partition numbers 1"
        },
        {
          "type": "ESCAPE",
          "content": "\\(en"
        },
        {
          "type": "TEXT",
          "content": "4 are given to the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "four partitions described in the MBR (the \"primary\" partitions),"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "regardless of whether they are unused or extended."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Thus, the first logical partition will be"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".BI",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hd X 5"
        },
        {
          "type": "ESCAPE",
          "content": "\\c"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "ESCAPE",
          "content": "\\&"
        },
        {
          "type": "TEXT",
          "content": "."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Both DOS-type partitioning and BSD-disklabel partitioning are supported."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "You can have at most 63 partitions on an IDE disk."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "For example,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /dev/hda"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "refers to all of the first IDE drive in the system; and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /dev/hdb3"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "refers to the third DOS \"primary\" partition on the second one."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "They are typically created by:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " +4n"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hda b 3 0"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hda1 b 3 1"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hda2 b 3 2"
        }
      ],
      [
        {
          "type": "ESCAPE",
          "content": "\\&"
        },
        {
          "type": "TEXT",
          "content": "..."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hda8 b 3 8"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hdb b 3 64"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hdb1 b 3 65"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hdb2 b 3 66"
        }
      ],
      [
        {
          "type": "ESCAPE",
          "content": "\\&"
        },
        {
          "type": "TEXT",
          "content": "..."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "mknod "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "m 660 /dev/hdb8 b 3 72"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "chown root:disk /dev/hd*"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".in",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " FILES"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /dev/hd*"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " SEE ALSO"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " chown (1),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " mknod (1),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " sd (4),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
