      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hpsa"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "is a SCSI driver for HP Smart Array RAID controllers."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " Options"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " \"hpsa_allow_any=1\" :"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This option allows the driver to attempt to operate on"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "any HP Smart Array hardware RAID controller,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "even if it is not explicitly known to the driver."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This allows newer hardware to work with older drivers."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Typically this is used to allow installation of"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "operating systems from media that predates the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "RAID controller, though it may also be used to enable"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hpsa"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "to drive older controllers that would normally be handled by the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " cciss (4)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "driver."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "These older boards have not been tested and are"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "not supported with"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hpsa ,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " cciss (4)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "should still be used for these."
        }
      ]
    ]
  },
