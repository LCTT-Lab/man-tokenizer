  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /sys/class/scsi_host/host*/rescan"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This is a write-only attribute."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Writing to this attribute will cause the driver to scan for"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "new, changed, or removed devices (e.g., hot-plugged tape drives,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or newly configured or deleted logical drives, etc.)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and notify the SCSI midlayer of any changes detected."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Normally a rescan is triggered automatically"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "by HP's Array Configuration Utility (either the GUI or the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "command-line variety);"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "thus, for logical drive changes, the user should not"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "normally have to use this attribute."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This attribute may be useful when hot plugging devices like tape drives,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or entire storage boxes containing preconfigured logical drives."
        }
      ]
    ]
  },
