      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /sys/class/scsi_host/host * /rescan"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The host"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " rescan"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "attribute is a write-only attribute."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Writing to this attribute will cause the driver to scan for new,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "changed, or removed devices (e.g., hot-plugged tape drives, or newly"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "configured or deleted logical drives) and notify the SCSI mid-layer of"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "any changes detected."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Usually this action is triggered automatically by configuration"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "changes, so the user should not normally have to write to this file."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Doing so may be useful when hot-plugging devices such as tape drives or"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "entire storage boxes containing pre-configured logical drives."
        }
      ]
    ]
  },
