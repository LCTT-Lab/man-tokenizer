  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TH",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " HOSTS 5 2017-09-15 \"Linux\" \"Linux Programmer's Manual\""
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NAME"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "hosts "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " static table lookup for hostnames"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " SYNOPSIS"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /etc/hosts"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " DESCRIPTION"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "This manual page describes the format of the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /etc/hosts"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "file."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This file is a simple text file that associates IP addresses"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "with hostnames, one line per IP address."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "For each host a single"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "line should be present with the following information:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".RS",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "IP_address canonical_hostname [aliases...]"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".RE",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Fields of the entry are separated by any number of blanks and/or"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "tab characters."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Text from a \"#\" character until the end of the line is"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "a comment, and is ignored."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Host names may contain only alphanumeric"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "characters, minus signs (\""
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": "\"), and periods (\".\")."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "They must begin with an"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "alphabetic character and end with an alphanumeric character."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Optional aliases provide for name changes, alternate spellings,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "shorter hostnames, or generic hostnames (for example,"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".IR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " localhost )."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The Berkeley Internet Name Domain (BIND) Server implements the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Internet name server for UNIX systems."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It augments or replaces the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /etc/hosts"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "file or hostname lookup, and frees a host from relying on"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /etc/hosts"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "being up to date and complete."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "In modern systems, even though the host table has been superseded by"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "DNS, it is still widely used for:"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " bootstrapping"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Most systems have a small host table containing the name and address"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "information for important hosts on the local network."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This is useful"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "when DNS is not running, for example during system bootup."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " NIS"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Sites that use NIS use the host table as input to the NIS host"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "database."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Even though NIS can be used with DNS, most NIS sites still"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "use the host table with an entry for all local hosts as a backup."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".TP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".B",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " isolated nodes"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Very small sites that are isolated from the network use the host table"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "instead of DNS."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "If the local information rarely changes, and the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "network is not connected to the Internet, DNS offers little"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "advantage."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " FILES"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /etc/hosts"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " NOTES"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Modifications to this file normally take effect immediately,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "except in cases where the file is cached by applications."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SS",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " Historical notes"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "RFC"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "952 gave the original format for the host table, though it has"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "since changed."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Before the advent of DNS, the host table was the only way of resolving"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "hostnames on the fledgling Internet."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Indeed, this file could be"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "created from the official host data base maintained at the Network"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Information Control Center (NIC), though local changes were often"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "required to bring it up to date regarding unofficial aliases and/or"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "unknown hosts."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The NIC no longer maintains the hosts.txt files,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "though looking around at the time of writing (circa 2000), there are"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "historical hosts.txt files on the WWW."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "I just found three, from 92,"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "94, and 95."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " EXAMPLE"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": ".EX"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "# The following lines are desirable for IPv4 capable hosts"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "127.0.0.1       localhost"
        }
      ]
    ]
  },
  {
    "type": "empty",
    "lines": [
      [

      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "# 127.0.1.1 is often used for the FQDN of the machine"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "127.0.1.1       thishost.mydomain.org  thishost"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "192.168.1.10    foo.mydomain.org       foo"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "192.168.1.13    bar.mydomain.org       bar"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "146.82.138.7    master.debian.org      master"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "209.237.226.90  www.opensource.org"
        }
      ]
    ]
  },
  {
    "type": "empty",
    "lines": [
      [

      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "# The following lines are desirable for IPv6 capable hosts"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "::1             localhost ip6-localhost ip6-loopback"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "ff02::1         ip6-allnodes"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "ff02::2         ip6-allrouters"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": ".EE"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".SH",
          "translatable": true
        },
        {
          "type": "TEXT",
          "content": " SEE ALSO"
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hostname (1),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " resolver (3),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " host.conf (5),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " resolv.conf (5),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " resolver (5),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " hostname (7),"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " named (8)"
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
      [
        {
          "type": "MACRO_BLOCK",
          "content": ".PP",
          "translatable": false
        }
      ]
    ]
  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "Internet RFC"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "952"
        }
      ]
    ]
  },
  {
    "type": "comment",
    "lines": [
      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\" .SH AUTHOR"
        }
      ],
      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\" This manual page was written by Manoj Srivastava <srivasta@debian.org>,"
        }
      ],
      [
        {
          "type": "COMMENT_BLOCK",
          "content": ".\\\" for the Debian GNU/Linux system."
        }
      ]
    ]
  }
]
