  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "This file is read by the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " route (8)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "and"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " netstat (8)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "utilities."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Only Class A, B or C networks are supported, partitioned networks"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "(i.e., network/26 or network/28) are not supported by this facility."
        }
      ]
    ]
  },
