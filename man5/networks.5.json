  },
  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "networks "
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": " network name information"
        }
      ]
