          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " getgrent (3)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "call respectively."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Otherwise, each"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " getpwent (3)"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "or"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".BR",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " getgrent (3)"
        }
      ],
      [
        {
          "type": "TEXT",
