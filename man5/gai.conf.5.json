        {
          "type": "TEXT",
          "content": "R"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Add another rule to the RFC"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "3484 scope table for IPv4 address."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "By default, the scope IDs described in section 3.2 in RFC"
        },
        {
          "type": "ESCAPE",
          "content": "\\ "
        },
        {
          "type": "TEXT",
          "content": "3438 are used."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "Changing these defaults should hardly ever be necessary."
        }
      ]
    ]
  },
