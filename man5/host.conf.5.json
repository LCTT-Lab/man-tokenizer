  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The file"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " /etc/host.conf"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "contains configuration information specific to the resolver library."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "It should contain one configuration keyword per line, followed by"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "appropriate configuration information."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The following keywords are recognized:"
        }
