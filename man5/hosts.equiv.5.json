  {
    "type": "content",
    "lines": [
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " username"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "entry grants a specific user access to all user"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "accounts (except root) without supplying a password."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "That means the"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "user is NOT restricted to like-named accounts."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "The"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " username"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "may"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "be (optionally) preceded by a plus (+) sign."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "You can also explicitly"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "deny access to a specific user by preceding the"
        }
      ],
      [
        {
          "type": "MACRO_INLINE",
          "content": ".I",
          "translatable": false
        },
        {
          "type": "TEXT",
          "content": " username"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "with"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "a minus ("
        },
        {
          "type": "ESCAPE",
          "content": "\\-"
        },
        {
          "type": "TEXT",
          "content": ") sign."
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "This says that the user is not trusted no matter"
        }
      ],
      [
        {
          "type": "TEXT",
          "content": "what other entries for that host exist."
        }
      ]
    ]
  },
  {
    "type": "macro",
    "lines": [
